#include<stdio.h>
int main()
{
    int a[50],i,k,sum;
    printf("enter the range ");
    scanf("%d",&i);
    for(k=0;k<i;k++)
    {
        printf("enter the elements");
        scanf("%d",&a[k]);
    }
    for (k=0;k<i;k++)
    {
        printf("%d",a[k]);
    }
    for(k=0;k<i;k++)
    {
        sum=sum+a[k];
    }
    printf("%d",sum);
    
}
